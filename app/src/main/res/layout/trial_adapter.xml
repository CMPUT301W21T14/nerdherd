<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/trial_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:text="oS5RUC2W6h8enhhUfkzU"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/trials_executed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:text="48.5"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/trial_user"
        app:layout_constraintLeft_toRightOf="@+id/trial_user"
        app:layout_constraintTop_toTopOf="@+id/trial_user" />


</androidx.constraintlayout.widget.ConstraintLayout>